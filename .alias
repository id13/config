alias wifi='sudo ip link set wlp2s0 down &&  sudo wifi-menu wlp2s0'
alias emacs='emacs -nw'
alias ls='ls --color=auto'
alias ll='ls --color=auto -al'
alias less='less -R'
alias edit='emacs'
alias subl='/opt/sublime_text_3/sublime_text'
alias wrefresh="sudo ip link set wlp2s0 down && sudo ip link set wlp2s0 up && sudo wifi-menu"
alias music='systemctl --user restart mopidy && ncmpcpp'
alias sucre='yaourt --noconfirm'
alias rake='noglob rake'
alias cap='noglob cap'
